<h1>Edit Song Form</h1>

<%= form_for(@song) do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br><br>

    <%= f.label :artist %><br>
    <%= f.text_field :artist_id %><br><br>

    <%= f.label :genre %><br>
    <%= f.text_field :genre_id %><br><br>  

    <%= f.submit %>
<% end %>